{
  "name": "react-course-backend",
  "version": "0.1.0",
  "description": "Backend for React.js course",
  "scripts": {
    "start": "node server.js",
    "dev_windows": "node server.js",
    "prod_windows": "set NODE_ENV=PROD&node server.js",
