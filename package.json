    "es6-promise": "^3.0.2",
    "express": "^4.13.3",
    "flux": "^2.1.1",
    "keymirror": "^0.1.1",
    "lodash": "3.10.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "devDependencies": {
    "react-hot-loader": "^1.3.0",
    "babel-eslint": "^4.1.3",
    "css-loader": "^0.20.2",
    "eslint": "^1.7.3",
    "eslint-loader": "^1.1.0",
