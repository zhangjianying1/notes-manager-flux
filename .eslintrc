{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "blockBindings": true,
    "arrowFunctions": true,
    "blockBindings": true,
    "classes": true,
    "modules": true,
    "restParams": true,
    "defaultParams": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties":true
  },
  "rules": {
    "no-var": [1],
    "new-cap": 1,
    "strict": 1,
    "no-debugger": 2,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "comma-dangle": [2, "never"],
    "no-dupe-args": 2,
    "no-func-assign": 2,
    "eol-last": 0,
    "quotes": [2, "single"],
    "jsx-quotes": 1,
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1
  }
}